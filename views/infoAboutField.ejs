        <a class="text_sup" href="">example@mail.com</a>
    </div>
</section>
<section class="main_container ">
    <div class="date_container">
        <p id="today" class="text_date">12.12.22</p>
    </div>
    <div>
        <div class="main-flex">
            <div class="column">
                <h3>Информация о поле</h3>
            </div>
            <div class="column">
                <div class="row">
                    <p>ID поля</p>
                    <p><%= field.id %></p>
                </div>
                <div class="row">
                    <p>Владелец поля</p>
                    <p><%= field.emailOwner %></p>
                </div>
                <div class="row">
                    <p>Имя</p>
                    <p><%= field.name %></p>
                </div>
                <div class="row">
                    <p>Широта</p>
                    <p><%= field.latitude %></p>
                </div>
                <div class="row">
                    <p>Долгота</p>
                    <p><%= field.longitude %></p>
                </div>
                <div class="row">
                    <p>Площадь</p>
                    <p><%= field.square %> га</p>
                </div>

                <div class="row">
                    <p>Количество удобрения</p>
                    <p><%= field.countFert %> л</p>
                </div>

                <div class="row">
                    <p>Количество машин</p>
                    <p><%= field.countCars %></p>
                </div>
            </div>

        </div>
    </div>
    <div class="nav_calendar_container">
        <div class="field_select_container">
            <select default="Выберите поле" class="field_select" name="" id="">
                <option value="">Выберите поле</option>
            </select>
        </div>
        <div class="title_text_container">
            <p class="title_text">Календарь</p>
        </div>
        <div class="arrow_month">
            <button id="select_month_left">
                <svg class="selector_month" width="512px" height="512px" viewBox="-32 0 512 512"
                     xmlns="http://www.w3.org/2000/svg">
                    <path d="M257.5 445.1l-22.2 22.2c-9.4 9.4-24.6 9.4-33.9 0L7 273c-9.4-9.4-9.4-24.6 0-33.9L201.4 44.7c9.4-9.4 24.6-9.4 33.9 0l22.2 22.2c9.5 9.5 9.3 25-.4 34.3L136.6 216H424c13.3 0 24 10.7 24 24v32c0 13.3-10.7 24-24 24H136.6l120.5 114.8c9.8 9.3 10 24.8.4 34.3z"/>
                </svg>
            </button>
            <div class="monthNumber" id="monthNumber"></div>
            <button id="select_month_right">
                <svg class="selector_month" width="512px" height="512px" viewBox="-32 0 512 512"
                     xmlns="http://www.w3.org/2000/svg">
                    <path d="M190.5 66.9l22.2-22.2c9.4-9.4 24.6-9.4 33.9 0L441 239c9.4 9.4 9.4 24.6 0 33.9L246.6 467.3c-9.4 9.4-24.6 9.4-33.9 0l-22.2-22.2c-9.5-9.5-9.3-25 .4-34.3L311.4 296H24c-13.3 0-24-10.7-24-24v-32c0-13.3 10.7-24 24-24h287.4L190.9 101.2c-9.8-9.3-10-24.8-.4-34.3z"/>
                </svg>
            </button>
        </div>
    </div>

    <div class="calendar_container">
        <div id="calendar" class="calendar"></div>
    </div>

</section>
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="../../calendar/script.js"></script>
</body>
</html>